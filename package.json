{
  "name": "emotebox",
  "version": "1.0.0",
  "description": "Discord emoji manager",
  "author": {
    "name": "khai93",
    "url": "https://github.com/khai93"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "test:client": "cd client && yarn test && cd ..",
    "test:server": "mocha ./server/**/**/*.test.js",
    "test": "yarn test:server && yarn test:client",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "node server/server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "aws-sdk": "^2.749.0",
    "body-parser": "^1.19.0",
    "celebrate": "^12.2.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "http-proxy-middleware": "^1.0.5",
    "jsdoc": "^3.6.6",
    "mocha": "^8.1.3",
    "mongoose": "^5.10.3",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "rand-token": "^1.0.1",
    "rate-limit-mongo": "^2.3.0",
    "react-images-upload-demo": "https://github.com/khai93/react-images-upload.git#fix/save-state",
    "react-modal": "^3.11.2",
    "react-progress-button": "^5.1.0",
    "react-pure-loaders": "^3.0.1",
    "react-router-dom": "^5.2.0",
    "react-tabs": "^3.1.1",
    "react-test-renderer": "^16.13.1",
    "sinon": "^9.1.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khai93/emotebox"
  },
  "bugs": {
    "url": "https://github.com/khai93/emotebox/issues"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ]
}
